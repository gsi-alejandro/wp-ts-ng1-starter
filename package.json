    "@types/jest": "^18.1.1",
    "@types/jquery": "^2.0.40",
    "@types/lodash": "^4.14.40",
    "@types/node": "^7.0.5",
    "angular-mocks": "^1.6.1",
    "awesome-typescript-loader": "^3.0.0-beta.9",
    "chalk": "^1.1.3",
    "copyfiles": "^1.0.0",
